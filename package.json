{
  "name": "postgres_udacity",
  "version": "0.1.0",
  "description": "This is my road to learn building servers with postgres and express",
  "main": "server.ts",
  "scripts": {
    "prettier": "prettier --config .prettierrc ./src/**/**/*.ts --write",
    "start": "nodemon src/server.ts",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "test": "npx tsc && db-migrate --env test up && jasmine-ts && db-migrate db:drop test",
    "tsc": "tsc",
    "lint": "eslint ./src/**/*.ts",
    "build": "npx tsc"
  },
  "author": "Udacity",
  "license": "ISC",
